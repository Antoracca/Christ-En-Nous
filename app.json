{
  "expo": {
    "name": " ",
    "slug": "Christ En Nous",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/image1.png",
    "scheme": "christennous",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "Cette application utilise Face ID pour une connexion rapide et sécurisée.",
        "CFBundleDisplayName": "Christ En Nous"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.christennous",
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/image1.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/image1.png",
            "backgroundColor": "#0F172A"
          }
        }
      ],
      "expo-font",
      "expo-localization",
      "expo-secure-store",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Christ En Nous to use Face ID for secure authentication."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "5104b94d-a5c8-4754-b8b3-b548bf72e3fe"
      }
    },
    "owner": "koueni12"
  }
}